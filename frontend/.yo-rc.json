{
  "frontend": {
    "configuration": {
      "config": {
        "configName": "config",
        "topScope": [
          "const path = require('path');",
          "const webpack = require('webpack');",
          "\n"
        ],
        "webpackOptions": {
          "mode": "'development'",
          "entry": "'./src/index.js'",
          "output": {
            "path": "path.resolve(__dirname, 'public')"
          },
          "plugins": [
            "new webpack.ProgressPlugin()"
          ],
          "module": {
            "rules": [
              {
                "test": "/\\.(js|jsx)$/",
                "include": [],
                "loader": "'babel-loader'"
              },
              {
                "test": "/.(sa|sc|c)ss$/",
                "use": [
                  {
                    "loader": "\"style-loader\""
                  },
                  {
                    "loader": "\"css-loader\"",
                    "options": {
                      "sourceMap": true
                    }
                  },
                  {
                    "loader": "\"sass-loader\"",
                    "options": {
                      "sourceMap": true
                    }
                  }
                ]
              }
            ]
          },
          "devServer": {
            "open": true,
            "host": "'localhost'"
          }
        }
      }
    }
  }
}
